		{
			"title": "alert",
			"buttons": [
				{
					"value": "成功提示",
					"callback": "$.alert('成功提示', {type:'success'});"
				},
				{
					"value": "错误提示",
					"callback": "$.alert('错误提示', {type:'error'});"
				},
				{
					"value": "确定提示",
					"callback": "$.alert('是否确定删除此记录？', {type:'confirm', callback:function() {console.log('执行：确认提示回调函数')}});"
				},
				{
					"value": "确定提示有title",
					"callback": "$.alert('是否确定删除此记录？', {type:'confirm', title:'确认提示',callback:function() {console.log('执行：确认提示回调函数')}});"
				},
				{
					"value": "确定提示有title手动关闭",
					"callback": "$.alert('是否确定删除此记录？', {type:'confirm', title:'确认提示',callbackManual:function(cb) {setTimeout(function(){ cb.closeFun(); }, 1000);}});"
				},
				{
					"value": "确定提示有title手动关闭，失败提示",
					"callback": "$.alert('是否确定删除此记录？', {type:'confirm', title:'确认提示',callbackManual:function(cb) {setTimeout(function(){ cb.msgFun('处理失败'); }, 1000);}});"
				}
			]
			
		},
